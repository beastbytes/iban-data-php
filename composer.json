{
    "name": "beastbytes/iban-formats",
    "type": "library",
    "description": "International Bank Account Number (IBAN) format data indexed by ISO 3166 country alpha-2 codes",
    "keywords": [
        "International Bank Account Number",
        "IBAN"
    ],
    "support": {
        "issues": "https://github.com/beastbytes/iban-formats/issues",
        "source": "https://github.com/beastbytes/iban-formats"
    },
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Chris Yates",
        "email": "chris.l.yates@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\Iban\\Formats\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
