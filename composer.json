{
    "name": "beastbytes/iban-data-php",
    "type": "library",
    "description": "Implementation of iban-data to retrieve IBAN (International Bank Account Number)] from PHP storage.",
    "keywords": [
        "International Bank Account Number",
        "IBAN"
    ],
    "support": {
        "issues": "https://github.com/beastbytes/iban-data-php/issues",
        "source": "https://github.com/beastbytes/iban-data-php"
    },
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Chris Yates",
        "email": "chris.l.yates@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "beastbytes/iban-data": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\IBAN\\PHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "infection": "roave-infection-static-analysis-plugin --log-verbosity=all --debug",
        "psalm": "psalm",
        "robo": "robo",
        "test": "phpunit --testdox"
    }
}
